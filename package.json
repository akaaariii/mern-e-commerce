{
  "name": "mern-e-commerce",
  "version": "1.0.0",
  "description": "MERN shopping cart app",
  "main": "index.js",
  "scripts": {
    "start": "node backend/index.js",
    "server": "nodemon backend/index.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akaaariii/0520-node-final-project.git"
  },
  "author": "Akari Tsutsui",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/akaaariii/0520-node-final-project/issues"
  },
  "homepage": "https://github.com/akaaariii/0520-node-final-project#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-session": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "mongoose": "^5.11.15",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "stripe": "^8.135.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7",
    "prettier": "2.2.1"
  }
}
